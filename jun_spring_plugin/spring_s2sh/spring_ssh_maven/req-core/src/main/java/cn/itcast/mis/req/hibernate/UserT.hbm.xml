<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="cn.itcast.mis.req.hibernate.UserT" table="UserT">
<id name="userId"
        type="java.lang.Integer"
        column="User_ID">
        <meta attribute="scope-set">protected</meta>
        <generator class="identity" />
</id>
<property name="userName" column="User_Name" type="java.lang.String" length="50"/>
<property name="userPassword" column="User_Password" type="java.lang.String" length="200"/>
<property name="userEmail" column="User_Email" type="java.lang.String" length="200"/>
</class>
</hibernate-mapping>
