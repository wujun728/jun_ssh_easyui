			<plugin>
				<artifactId>maven-enforcer-plugin</artifactId>
				<dependencies>
					<dependency>
						<groupId>com.qunar.ams.maven.enforcer</groupId>
						<artifactId>enforcer-custom-rules</artifactId>
						<version>1.0.0</version>
						<scope>compile</scope>
					</dependency>
					<dependency>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>extra-enforcer-rules</artifactId>
						<version>1.0-alpha-3</version>
						<scope>compile</scope>
					</dependency>
				</dependencies>
				<configuration>
					<rules>
						<bannedDependencies>
							<searchTransitive>true</searchTransitive>
							<excludes>
								<exclude>commons-logging</exclude>
								<exclude>org.slf4j:*:(,1.6)</exclude>
							</excludes>
						</bannedDependencies>
						<evaluateBeanshell>
							<condition>print("[INFO] [Qunar Enforcer Rules] super-pom ");1==1</condition>
						</evaluateBeanshell>
						<requireGroupId implementation="com.qunar.maven.enforcer.rule.requireGroupId">
							<shouldIskip>false</shouldIskip>
						</requireGroupId>
						<requireVersion implementation="com.qunar.maven.enforcer.rule.requireVersion">
							<shouldIskip>false</shouldIskip>
						</requireVersion>
						<requireDistributionManagement
							implementation="com.qunar.maven.enforcer.rule.requireDistributionManagement">
							<shouldIskip>false</shouldIskip>
							<repositoryId>releases</repositoryId>
							<snapshotRepositoryId>snapshots</snapshotRepositoryId>
						</requireDistributionManagement>
						<requirePlugins implementation="com.qunar.maven.enforcer.rule.requirePlugins">
							<shouldIskip>false</shouldIskip>
							<artifactIds>
								<artifactId>maven-source-plugin</artifactId>
							</artifactIds>
						</requirePlugins>
						<checkDeployRelease
							implementation="com.qunar.maven.enforcer.rule.checkDeployRelease">
							<shouldIskip>${checkDeployRelease_skip}</shouldIskip>
						</checkDeployRelease>
						<banDuplicateClasses>
							<findAllDuplicates>true</findAllDuplicates>
							<ignoreClasses>
								<ignoreClass>junit.*</ignoreClass>
								<ignoreClass>org.junit.*</ignoreClass>
								<ignoreClass>org.w3c.dom.*</ignoreClass>
								<ignoreClass>javax.xml.namespace.*</ignoreClass>
								<ignoreClass>org.apache.axis2.*</ignoreClass>
							</ignoreClasses>
							<message>[ERROR] [Qunar Enforcer Rules] find DuplicateClasses</message>
						</banDuplicateClasses>
					</rules>
				</configuration>
			</plugin>
